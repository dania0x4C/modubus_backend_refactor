    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.19",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-command": "^3.1.5",
    "nodemailer": "^6.9.11",
    "pg": "^8.16.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
