{
  "name": "modubus-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "build": "nest build",
    "start": "nest start",
    "docker:local": "bash scripts/start_local.sh",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9230 --watch",
    "start:prod": "node dist/main",
    "db:migrate": "docker exec modubus-backend npx nestjs-command db:migrate",
    "db:sync": "docker exec modubus-backend npx nestjs-command db:sync",
    "db:revert": "docker exec modubus-backend npx nestjs-command db:revert",
    "db:show": "docker exec modubus-backend npx nestjs-command db:show",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:test": "docker compose up --build",
    "seed": "docker compose exec modubus-backend npx ts-node src/common/testSeeding/seed.module.ts",
    "git:log": "git log --oneline --all --exclude=refs/stash"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
